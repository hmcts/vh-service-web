{
  "stryker-config": {
    "reporters": [
      "Html",
      "Progress",
      "ClearText"
    ],
    "log-level": "info",
    "timeout-ms": 10000,
    "log-file": true,
    "project-file": "ServiceWebsite.csproj",
    "max-concurrent-test-runners": 4,
    "threshold-high": 80,
    "threshold-low": 70,
    "threshold-break": 40,
    "excluded-mutations": [
    ],
    "ignore-methods": [
      "*TraceException"
    ],
    "mutate": [
      "!Startup.cs",
      "!Program.cs",
      "!ConfigureServicesExtensions.cs",
      "!Pages/Error.cshtml.cs",
      "!Swagger/AddBearerAuthHeaderFilter.cs",
      "!Swagger/AuthResponsesOperationFilter.cs",
      "!Helpers/ExceptionMiddleware.cs",
      "!Security/AddBearerTokenHandler.cs",
      "!Security/BaseServiceTokenHandler.cs",
      "!Security/BookingsApiTokenHandler.cs",
      "!Security/TokenProvider.cs",
      "!Security/UserApiTokenHandler.cs"
    ]
  }
}