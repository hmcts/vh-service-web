{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Microsoft": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "Authority": "https://login.microsoftonline.com/",
    "RedirectUri": "https://localhost:5600/home",
    "PostLogoutRedirectUri": "https://localhost:5600/logout"
  },
  "RequireHttpsMetadata": false,
  "TokenCacheExpiresIn": 20,
  "AppInsightsKey": "AppInsightsKey",
  "VhServices": {
    "EnableMobileSupport":  true
  }
}